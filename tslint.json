{
  "rules": {
    "curly": true,
    "class-name": true,
    "jsdoc-format": true,
    "no-duplicate-variable": true,
    "no-var-keyword": true,
    "no-empty": true,
    "no-unused-expression": true,
    "no-var-requires": true,
    "no-require-imports": true
  },
  "array-type": [true, "array-simple"]
}
